    <sequenceFlow id="部门经理审批" name="报销金额大于500，小于等于1000" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${money >500 && money <=1000}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="默认执行财务" name="报销金额小于500，默认执行" sourceRef="exclusivegateway1" targetRef="usertask3"></sequenceFlow>
    <userTask id="usertask5" name="审批（总经理）" activiti:assignee="总经理_老王"></userTask>
